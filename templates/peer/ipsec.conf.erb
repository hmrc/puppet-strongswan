#
# strongSwan IPsec configuration file.
#
# Peer connection name: <%= @conn_name %>.
#

conn <%= @conn_name %>
    ike=<%= @ike %>
    esp=<%= @esp %>
    keyexchange=<%= @keyexchange %>
    keyingtries = <%= @keyingtries %>
    ikelifetime=<%= @ikelifetime %>
    lifetime=<%= @lifetime %>
    margintime=<%= @margintime %>
    closeaction=<%= @closeaction %>
    dpdaction=<%= @dpdaction %>
    compress=<%= @compress %>
    left=<%= @left %>
    leftcert=<%= @leftcert %>
    leftid="<%= @leftid %>"
    leftfirewall=<%= @leftfirewall %>
    leftsubnet=<%= @leftsubnet %>
    right=<%= @right %>
    rightauth=<%= @rightauth %>
    rightid="<%= @rightid %>"
    rightsubnet=<%= @rightsubnet %>
    auto=<%= @auto %>
